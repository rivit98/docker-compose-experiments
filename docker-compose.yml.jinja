services:
  {% for task_name, task_details in tasks %}
  {{ task_name }}:
    build:
      context: ./{{ task_name }}/private
    ports:
      - "{{ task_details.get('port') }}:1337"
    privileged: true
    restart: unless-stopped
    {% if task_details.get('enabled') -%}
    profiles:
        - enabled
    {% endif %}
  {%- endfor %}
